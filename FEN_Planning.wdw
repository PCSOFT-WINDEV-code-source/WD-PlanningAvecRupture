         PROCEDURE AvantCréation(rdvCréé est un RendezVous)
         
         // Mémorise le rendez-vous qui vient d'être créé
         // (s'il est déplacé sur une ressource "rupture", il faudra le repositionner)
         gRDVTemp = rdvCréé
         
         // Mémorise le rendez-vous
         hraz(TACHE)
         TACHE.DATEDEBUT = rdvCréé..DateDébut
         TACHE.datefin = rdvCréé..datefin
         TACHE.LIBTACHE = rdvCréé..Titre
         TACHE.IDPERSONNE = extraitchaine(rdvCréé..Ressource, 2, ESC)
         si hajoute(tache) = faux ALORS
         	Erreur("Impossible de mémoriser le rendez-vous en base", herreurinfo(herrmessage))
         	renvoyer faux
         FIN
         
         // Mémorise l'identifiant du rendez-vous
         rdvCréé..Id = tache.IDTACHE
         
         // Modifie la catégorie
         hlitrecherchepremier(PERSONNE, IDPERSONNE, ExtraitChaîne(rdvCréé..Ressource, 2, ESC))
         si htrouve(personne)
         	rdvCréé..Catégorie = personne.idgroupe
         Fin
         
         renvoyer vrai
        type : 135
