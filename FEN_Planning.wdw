      // Résumé : Ajouter une ressource "Rupture"
      // Syntaxe :
      //[ <Résultat> = ] PlanningAjouteRessourceRupture (<sNomChampPlanning> est chaîne, <sRessource> est chaîne [, <nCouleur> est entier])
      //
      // Paramètres :
      //	sNomChampPlanning (chaîne ANSI) : Nom complet du champ Planning
      //	sRessource (chaîne ANSI) : Ressource (libellé contenant éventuellement un gLien)
      //	nCouleur (entier - valeur par défaut=8421504) : Couleur à appliquer à la "rupture"
      // Valeur de retour :
      // 	entier : Indice de la ressource ajoutée
      //// Traitement automatique des erreurs : RENVOYER -1
      //
      
      PROCEDURE PlanningAjouteRessourceRupture(LOCAL sNomChampPlanning est une chaine, LOCAL sRessource est une chaine, LOCAL nCouleur est un entier = RVB(190, 191, 191)) : entier
      
      nIndice 	est un entier
      sID			est une chaine
      sLibellé	est une chaine
      
      
      // Le champ Planning doit exister
      si ChampExiste(sNomChampPlanning) = faux ALORS
      	erreurconstruit("Le champ Planning %1 n'existe pas", sNomChampPlanning)
      	renvoyer -1
      FIN
      
      // Définit le libellé et l'identifiant de la ressource (si la ressource précisée contient un gLien)
      si position(sRessource, ESC+"K") > 0 ALORS
      	
      	// Récupère le libellé et l'identifiant (gLien) en fonction de la position du gLien
      	si extraitchaine(sRessource, 1, ESC+"K") ~= "" ALORS
      		// Libellé
      		sLibellé 	= ExtraitChaîne(sRessource, 3, ESC+"K")
      		// Identifiant
      		sID 		= ExtraitChaîne(sRessource, 2, ESC+"K")
      	SINON
      		// Libellé
      		sLibellé 	= ExtraitChaîne(sRessource, 1, ESC+"K")
      		// Identifiant
      		sID 		= ExtraitChaîne(sRessource, 2, ESC+"K")
      	FIN
      	
      SINON // pas de gLien
      	
      	// Le libellé et l'identifiant sont identiques
      	sID 		= sRessource
      	sLibellé 	= sRessource
      	
      FIN
      
      // Ajoute la ressource
      nIndice = PlanningAjouteRessource(sNomChampPlanning, sRessource)
      
      // Modifie l'affichage du groupe pour "simuler" une rupture (hauteur réduite, couleur différente)
      {sNomChampPlanning, indchamp}..Ressource[sID]..Hauteur 				= 20
      {sNomChampPlanning, indChamp}..Ressource[sID]..CouleurFond 			= nCouleur
      {sNomChampPlanning, indChamp}..Ressource[sID]..CouleurFondTitre 	= nCouleur
      
      // S'il s'agit du premier chargement : mémorise la "rupture" comme déroulée par défaut
      SI gtaEnroulementRupture[sID]..Vide ALORS gtaEnroulementRupture[sID] = Vrai
      
      // Selon l'enroulement de la "rupture" à appliquer
      SELON gtaEnroulementRupture[sID]
      	// Déroulée
      	CAS vrai
      		// Ajoute un picto déroulé au titre
      		{sNomChampPlanning, indChamp}..Ressource[sID]..Titre = gImage(IMG_Déroulé) +" "+ sLibellé
      		
      	// Enroulée
      	CAS faux
      		// Ajoute un picto enroulé au titre
      		{sNomChampPlanning, indChamp}..Ressource[sID]..Titre = gImage(IMG_Enroulé) +" "+ sLibellé
      		
      FIN
      
      renvoyer nIndice
