  procedures :
   -
     name : RemplitPlanning
     procedure_id : 1269134673872241081
     type_code : 14
     code : |1+
      // Résumé : Remplit le planning
      // Syntaxe :
      //RemplitPlanning ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE RemplitPlanning()
      
      sIDGroupe 				est une chaine
      sIDPersonne 			est une chaine
      sRessourceAffichée		est une chaine
      
      
      // Stoppe l'affichage du champ (pour éviter l'effet de clignotement au remplissage)
      PLN_PlanningRupture..AffichageActif = faux
      
      // Mémorise la ressource actuellement affichée (pour repositionner le planning dessus)
      sRessourceAffichée = PlanningPositionRessource(PLN_PlanningRupture)
      
      // Vide le planning complètement
      PlanningSupprimeTout(PLN_PlanningRupture)
      
      // Parcourt les groupes
      POUR TOUT GROUPE SUR LIBGROUPE
      	
      	// Le groupe est identifié par G[ESC]<IDGROUPE>
      	sIDGroupe = "G" +ESC+ GROUPE.IDGROUPE
      	
      	// Ajoute le groupe
      	PlanningAjouteRessourceRupture(PLN_PlanningRupture..nomcomplet, gLien(sIDGroupe) + GROUPE.LIBGROUPE, (INT_CouleurRupture ? donnecouleur(groupe.idgroupe) SINON *))
      	
      	// Ajoute les personnes de ce groupe
      	POUR TOUT PERSONNE SUR NOM AVEC "IDGroupe = " + GROUPE.IDGROUPE
      		
      		// La personne est identifiée par P[ESC]<IDPERSONNE>
      		sIDPersonne = "P" +ESC+ PERSONNE.IDPERSONNE
      		
      		// Ajoute la personne
      		PlanningAjouteRessourceFille(PLN_PlanningRupture..NomComplet, sIDGroupe, PERSONNE.NOM +RC+ gPoliceItalique(Vrai) + "("+PERSONNE.POSTE+")" + gPoliceItalique(Faux) + gLien(sIDPersonne))
      		
      	FIN
      	
      FIN
      
      // Parcourt les tâches
      POUR TOUT TACHE SUR DATEDEBUT
      	
      	// Recherche la personne liée à la tâche
      	HLitRecherchePremier(PERSONNE, IDPERSONNE, TACHE.IDPERSONNE)
      	si htrouve(personne) ALORS
      		
      		// Ajoute le rendez-vous
      		// /!\ Attention : la ressource ayant été ajoutée avec un gLien, c'est ce même gLien qu'il faut indiquer comme ressource pour la tâche
      		PlanningAjouteRendezVous(PLN_PlanningRupture..nomcomplet, "P"+ESC+PERSONNE.IDPERSONNE, TACHE.LIBTACHE, TACHE.DATEDEBUT, TACHE.DATEFIN, personne.IDGROUPE)	
      		
      	FIN
      	
      FIN
      
      // Positionne le planning sur la dernière ressource affichée mémorisée
      si sRessourceAffichée <> "" alors PlanningPositionRessource(PLN_PlanningRupture, sRessourceAffichée)
      
      // Réactive l'affichage du champ
      PLN_PlanningRupture..AffichageActif = vrai
     type : 458752
   -
     name : PlanningAjouteRendezVous
     internal_properties : 
     procedure_id : 1269439083994123963
     type_code : 14
     group : 2
     code : |1+
      // Résumé : Ajoute un rendez-vous au planning
      // Syntaxe :
      //[ <Résultat> = ] PlanningAjouteRendezVous (<sNomChampPlanning> est chaîne, <sRessource> est chaîne, <sTitre> est chaîne, <dhDateDébut> est dateheure, <dhDateFin> est dateheure [, <vCatégorie> est variant])
      //
      // Paramètres :
      //	sNomChampPlanning (chaîne ANSI) : Nom du champ planning
      //	sRessource (chaîne ANSI) : Ressource
      //	sTitre (chaîne ANSI) : Titre du rendez-vous
      //	dhDateDébut (dateheure) : Date et heure de début
      //	dhDateFin (dateheure) : Date et heure de fin
      //	vCatégorie (variant - valeur par défaut="") : Catégorie
      // Valeur de retour :
      // 	entier : Indice d'ajout
      //// Traitement automatique des erreurs : RENVOYER -1
      //
      
      PROCEDURE PlanningAjouteRendezVous(LOCAL sNomChampPlanning est une chaine, LOCAL sRessource est une chaine, LOCAL sTitre est une chaine, LOCAL dhDateDébut est une dateheure, LOCAL dhDateFin est dateheure, LOCAl vCatégorie est un variant = "") : entier
      
      
      // Si la ressource précisée pour le rendez-vous est une ressource fille
      si gtaRessourceFille[sRessource]..vide = faux ALORS
      	
      	// Si la ressource parente est enroulée
      	si PlanningEtatRessource(sNomChampPlanning, gtaRessourceFille[sRessource]) = ressEnroulée ALORS
      		
