      // Résumé : Ajoute une ressource fille de rupture
      // Syntaxe :
      //[ <Résultat> = ] PlanningAjouteRessourceFille (<sNomChampPlanning> est chaîne, <sIDParent> est chaîne, <sRessource> est chaîne)
      //
      // Paramètres :
      //	sNomChampPlanning (chaîne ANSI) : Nom complet du champ Planning
      //	sIDParent (chaîne ANSI) : Identifiant de la ressource parent (libellé ou gLien)
      //	sRessource (chaîne ANSI) : Ressource (libellé avec éventuellement un gLien)
      // Valeur de retour :
      // 	entier : Indice de la ressource 
      //// Traitement automatique des erreurs : RENVOYER -1
      //
      
      PROCEDURE PlanningAjouteRessourceFille(LOCAL sNomChampPlanning est une chaine, LOCAL sIDParent est une chaine, LOCAL sRessource est une chaine) : entier
      
      nIndice 	est un entier
      sLibellé	est une chaine
      sID			est une chaine
      
      
      // Le champ Planning doit exister
      SI ChampExiste(sNomChampPlanning) = Faux ALORS
      	ErreurConstruit("Le champ Planning %1 n'existe pas", sNomChampPlanning)
      	RENVOYER -1
      FIN
      
      
      // Définit le libellé et l'identifiant de la ressource (si la ressource précisée contient un gLien)
      SI Position(sRessource, ESC+"K") > 0 ALORS
      	
      	// Récupère le libellé et l'identifiant (gLien) en fonction de la position du gLien
      	SI ExtraitChaîne(sRessource, 1, ESC+"K") ~= "" ALORS
      		// Libellé
      		sLibellé 	= ExtraitChaîne(sRessource, 3, ESC+"K")
      		// Identifiant
      		sID 		= ExtraitChaîne(sRessource, 2, ESC+"K")
      	SINON
      		// Libellé
      		sLibellé 	= ExtraitChaîne(sRessource, 1, ESC+"K")
      		// Identifiant
      		sID 		= ExtraitChaîne(sRessource, 2, ESC+"K")
      	FIN
      	
      SINON // pas de gLien
      	
      	// Le libellé et l'identifiant sont identiques
      	sID 		= sRessource
      	sLibellé 	= sRessource
      	
      FIN
      
      // Mémorise pour le couple parent / fille
      gtaRessourceFille[sID] = sIDParent
      
      // Si le parent est enroulé, la ressource n'est pas ajoutée au planning
      si gtaEnroulementRupture[sIDParent] = faux alors renvoyer -1
      
      // Ajoute la ressource
      nIndice = PlanningAjouteRessource(sNomChampPlanning, sRessource)
      
      
      renvoyer nIndice
