         PROCEDURE DéplacementRessource(rdvRessourceDéplacé est un RendezVous)
         
         // Si le rendez-vous est déplacé sur une ligne de "rupture"
         SI PlanningEtatRessource(moimeme..nomcomplet, rdvRessourceDéplacé..Ressource) <> resserreur ALORS
         	
         	// Repositionne le rendez-vous dans sa période d'origine, pour sa ressource d'origine
         	rdvRessourceDéplacé..Ressource 	= gRDVTemp..Ressource
         	rdvRessourceDéplacé..DateDébut 	= gRDVTemp..DateDébut
         	rdvRessourceDéplacé..DateFin 	= gRDVTemp..DateFin
         
         FIN
        type : 139
      -
        code : |1-
         
        type : 176
      -
        code : |1+
         // Bouton gauche relâché = fin de déplacement
         gbDeplacementEnCours = Faux
        type : 152
      -
        code : |1-
         
         // GESTION DU CURSEUR
         
         // Récupère l'identifiant de la ressource survolée
         sIdRessource est une chaine = PlanningInfoXY(MoiMême, plnRessource, SourisPosX(), SourisPosY())
         selon PlanningEtatRessource(MoiMême..NomComplet, sIdRessource)
         	
         	// Il s'agit d'un groupe  ("rupture")
         	CAS ressEnroulée, ressDéroulée	
         		
         		// Empêche la sélection de période
         		MoiMême..SélectionPériode = Faux
         		
         		// Si le clic gauche est enfoncé
         		SI gbDeplacementEnCours ALORS
         			
         			// Change le curseur en curseur interdit
         			MoiMême..CurseurSouris = curInterdit
         			
         		SINON
         			
         			// Si le survol est sur la zone de ressource (à gauche)
         			SI SourisPosX() <= gnLargeurRessource _ET_ SourisPosY() > gnHauteurJour ALORS
         				
         				// Positionne le curseur main (pour indiquer qu'un clic est possible
         				MoiMême..CurseurSouris = curMainSystème
         				
         			SINON // Si le survol et sur la zone des rendez-vous
         				
         				// Positionne le curseur par défaut
         				MoiMême..CurseurSouris = curFlèche
         				
         			FIN
         			
         		FIN
         		
         	// Il s'agit d'une personne (ressource "non rupture")
         	CAS ressErreur
         		
         		// Active la sélection de période
         		MoiMême..SélectionPériode = Vrai
         		
         		// Si le survol est sur la zone de ressource (à gauche)
         		SI SourisPosX() <= gnLargeurRessource _ET_ SourisPosY() > gnHauteurJour ALORS
         			
         			// Positionne le curseur main (pour indiquer qu'un clic est possible
         			MoiMême..CurseurSouris = curMainSystème
         			
         		SINON // Si le survol et sur la zone des rendez-vous
         			
         			// Positionne le curseur par défaut
         			MoiMême..CurseurSouris = curFlèche
         			
         		FIN
         		
         	autres cas
         		
         FIN
        type : 66048
      -
        internal_properties : CAAAAAgAAACbXULZSXZuNo9Mn5rEj3BJA12MnOiN45vPMjMgCNXPz9FcbjhzkNFN2INDFEhmHX5XOrpQYNQSxN5dCP3wQM5kbZE0u/W8yJgkjqIcpuLoOouJ+NOqezZHAMitxOeRJ5SV1adRUqsaAvTGZzw=
        code : |1+
         
         // Bouton gauche enfoncé = un déplacement est en cours
         gbDeplacementEnCours = vrai
         
         // Récupère l'identifiant (glien) de la ressource cliquée
         sIDRessource est une chaine = PlanningInfoXY(MoiMême, plnRessource, SourisPosX(), SourisPosY())
         
         // Si le clic est réalisé sur la zone de ressource
         si SourisPosX() <= gnLargeurRessource _et_ SourisPosY() > gnHauteurJour ALORS
         	
         	// Selon le type de ressource (rupture ou non) et l'état d'enroulement
         	SELON PlanningEtatRessource(MoiMême..NomComplet, sIDRessource)
         	
         		// Il s'agit d'un groupe  ("rupture")
         		CAS ressEnroulée, ressDéroulée
         			// Inverse l'état d'enroulement
         			PlanningInverseEtatRessource(PLN_PlanningRupture..NomComplet, sIDRessource)
         
         			// Mémorise la ressource actuellement affichée (pour repositionner le planning dessus)
         			gsRessourceAffichée = PlanningPositionRessource(PLN_PlanningRupture)
         			
         			// Remplit à nouveau le planning
         			RemplitPlanning()
         			
         		// Il s'agit d'une personne (ressource "non rupture")
         		CAS ressErreur
         			
         			// Recherche la personne
         			hlitrecherchepremier(personne, IDPERSONNE, extraitchaine(sIDRessource, 2, ESC))
         			si htrouve(personne) ALORS
         				
         				// Affiche le numéro de téléphone de la personne
         				Info(personne.NOM +RC+ "Tél : "+personne.TELEPHONE)
         				
         			FIN
         			
         	FIN
         	
         FIN
        type : 66049
      -
        code : |1+
         // Bouton gauche relâché = fin de déplacement
         gbDeplacementEnCours = faux
         
         // Positionne le curseur par défaut
         MoiMême..CurseurSouris = curFlèche
        type : 66050
