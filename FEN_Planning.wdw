      // Résumé : Renvoie l'état (enroulé / déroulé) d'une ressource "rupture"
      // Syntaxe :
      //[ <Résultat> = ] PlanningEtatRessource (<sNomChampPlanning> est chaîne, <sIDRessource> est chaîne)
      //
      // Paramètres :
      //	sNomChampPlanning (chaîne ANSI) : Nom complet du champ Planning
      //	sIDRessource (chaîne ANSI) : Identifiant de la ressource
      // Valeur de retour :
      // 	énumération : Etat de la ressource
      //// Traitement automatique des erreurs :   exécuter le bloc de code CAS ERREUR
      //
      
      PROCEDURE PlanningEtatRessource(LOCAL sNomChampPlanning est une chaine, LOCAL sIDRessource est une chaine) : EEtatRessource
      
      
      // Le champ Planning doit exister
      SI ChampExiste(sNomChampPlanning) = Faux ALORS
      	ErreurConstruit("Le champ Planning %1 n'existe pas", sNomChampPlanning)
      	RENVOYER ressErreur
      FIN
      
      // Si la ressource n'est pas trouvée dans le tableau
      si gtaEnroulementRupture[sIDRessource]..vide alors
      	// Ressource non rupture
      	renvoyer resserreur
      FIN
      
      // Renvoie l'état d'enroulement
      SELON gtaEnroulementRupture[sIDRessource]
      	
      	// Enroulée
      	CAS Faux : renvoyer ressEnroulée
      	
      	// Déroulée
      	CAS Vrai : RENVOYER ressderoulée
      		
      FIN
      
      CAS ERREUR:
      		RENVOYER ressErreur
