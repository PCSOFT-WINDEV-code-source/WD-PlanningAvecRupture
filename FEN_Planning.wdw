         PROCEDURE Suppression(rdvSupprimé est un RendezVous)
         
         // Confirmation
         SI Dialogue("Voulez-vous vraiment supprimer ce rendez-vous ?", ["Oui - Supprimer","Non"], 1, 2, dlgIcôneQuestion) = 2 ALORS renvoyer faux
         
         // Recherche l'enregistrement en base
         HLitRecherchePremier(TACHE, IDTACHE, rdvSupprimé..ID)
         SI HTrouve(TACHE) ALORS
         	// Supprime ce rendez-vous
         	SI hsupprime(TACHE) = Faux ALORS
         		Erreur("Impossible de supprimer ce rendez-vous en base", HErreurInfo(hErrMessage))
         		RENVOYER Faux
         	FIN
         SINON
         	// Enregistrement non trouvé
         	erreur("Impossible de retrouver ce rendez-vous en base")
         	RENVOYER Faux
         FIN
         
         renvoyer vrai
